            <!-- BEGIN PAGE CONTAINER-->
            <div class="page-content">
                <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
                <div id="portlet-config" class="modal hide">
                    <div class="modal-header">
                        <button data-dismiss="modal" class="close" type="button"></button>
                        <h3>Widget Settings</h3>

                    </div>
                    <div class="modal-body">Widget settings form goes here</div>
                </div>
                <div class="clearfix"></div>
                <div class="content">
                    <ul class="breadcrumb">
                        <li>
                            <p>YOU ARE HERE</p>
                        </li> 
                        <li>{{#link-to 'campaign'}}Campaign1{{/link-to}} <a href="campaign.html" >Campaign</a>
                        </li>
                        <li>{{#link-to 'snprojectedit'}}sn 01{{/link-to}} <a href="ac_project_edit.html" class="active">Social Network Project Edit</a>
                        </li>
                    </ul>   

                    <div class="row">
                    <label for="multi" style="width:100%">Select Module:</label>
                    <select id="multi" style="width:100%" multiple>
                        <option value="all">All</option>
                        <option value="sn">SN</option>
                        <option value="sb">SB</option>
                        <option value="wp">WP</option>
                        <option value="sn_phpfox">Social Blogs PHPFox</option>
                        <option value="sn_elgg">Social Blogs Elgg</option>
                    </select>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <div class="grid simple ">
                                
                                <div class="grid-body">
                                    <form id="form_traditional_validation" action="#">                           
                                        <div class="form-group">
                                            <label class="form-label">Project Name</label>                                  
                                   

                                            <div class="input-with-icon  right">                                       
                                                <i class=""></i>
                                                {{input type="text" class="form-control" value=name}}
                                                                             
                                            </div>
                                        </div>
                                        
                                        <div class="form-actions">  
                                            <div class="pull-right">
                                                <button type="submit" class="btn btn-success btn-cons"><i class="icon-ok"></i>Save Project</button>

                                            </div>
                                        </div>
                                    </form>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <div class="grid simple ">
                               
                                <div class="grid-body">
                                    <ul class="nav nav-tabs" id="tab-01">
                                        <li><a href="#postinginfor">Posting Infor</a></li>
                                        <li class="active"><a href="#sitelist">Site List</a></li>
                                        <li><a href="#schedule">Schedule</a></li>
                                    </ul>
                                    <div class="tools"> <a href="javascript:;" class="collapse"></a> <a href="#grid-config" data-toggle="modal" class="config"></a> <a href="javascript:;" class="reload"></a> <a href="javascript:;" class="remove"></a> </div>
                                    <div class="tab-content">
                                        <div class="tab-pane" id="postinginfor">
                                            
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="grid simple ">
                                                        
                                                        <div class="grid-body no-border">                                  
                                                            <div class="form-group">
                                                                <label class="form-label">Keyword</label>                   
                                                                <div class="controls">
                                                                {{input type="text" class="form-control" value=keyword}}
                                                                </div>
                                                            </div>
                                                            <div class="form-group">                                                                
                                                                <div class="controls">
                                                                <button type="submit" class="btn btn-success btn-cons"><i class="icon-ok"></i>Start download content</button>
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label class="form-label">Title</label>                 
                                                                <div class="controls">
                                                                    {{input type="text" class="form-control" value=title}}
                                                                </div>
                                                            </div>
                                                            <div class="form-group">
                                                                <label class="form-label">Tags</label>                  
                                                                <div class="controls">
                                                                    {{input type="text" class="form-control" value=tags}}
                                                                </div>
                                                            </div>
                                                            
                                                            <div class="form-group">
                                                                <label class="form-label">Content</label>                   
                                                                <div class="controls">
                                                                    {{textarea rows=10 class="form-control" id="content" type="text" value=content1}}
                                                                </div>
                                                            </div>
                                                            
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="tab-pane active" id="sitelist">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div class="grid simple ">
                                                        
                                                        <div class="grid-body">  
                                                            <div class="form-group">                    
                                                                <div class="controls">
                                                                {{view Ember.Select
                                                                    content=modules}}
                                                                    
                                                                </div>
                                                            </div>
                                                            <div class="form-group">                    
                                                                <div class="controls">
                                                                {{view Ember.Select
                                                                    content=siteStatus}}
                                                                    
                                                                </div>
                                                            </div>
                                                            <button type="button" class="btn btn-default btn-cons">Filter</button>  
                                                            <div class="form-group">                                                                
                                                                <div class="row-fluid">
                                                                    <div class="checkbox check-default">
                                                                      <input id="checkboxCheck" type="checkbox" value="2">
                                                                      <label for="checkboxCheck">Check</label>
                                                                    </div>
                                                                </div>
                                                            </div>  
                                                                            
                                                            <table class="table no-more-tables">
                                                                <thead>
                                                                    <tr>
                                                                        <th style="width:1%" >
                                                                <div class="checkbox check-default">
                                                                    <input id="checkbox20" type="checkbox" value="20" class="checkall">
                                                                    <label for="checkbox20"></label>
                                                                </div>
                                                                </th>
                                                                <th style="width:10%">Site</th>
                                                                <th style="width:9%">Module</th>
                                                                <th style="width:6%">PR</th>
                                                                <th style="width:9%">Status</th>                                                                
                                                                </tr>
                                                                </thead>
                                                                <tbody>
                                                                {{#each site in sites}}
                                                                    <tr>
                                                                        <td class="v-align-middle">
                                                                            <div class="checkbox check-default">
                                                                                {{input type="checkbox" checked=site.checked}}
                                                                                <label></label>
                                                                            </div>
                                                                        </td>
                                                                        <td class="v-align-left"><span class="muted">{{site.name}}</span></td>
                                                                        <td class="v-align-left"><span class="muted">{{site.module}}</span></td>
                                                                        <td class="v-align-right"><span class="muted">{{site.pr}}</span></td>
                                                                        <td class="v-align-left"><span class="muted">{{site.status}}</span></td>
                                                                                        
                                                                    </tr>
                                                                {{/each}}
                                                                    
                                                                    

                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="tab-pane" id="schedule">                                            
                                            
                                        </div>
                                    
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>    
        </div>
